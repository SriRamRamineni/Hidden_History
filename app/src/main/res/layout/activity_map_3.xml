<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:mapbox="http://schemas.android.com/apk/res-auto"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.sriramramineni.routing_sample.Map_Activity3">
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
        <FrameLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:id="@+id/container">
            <!--<fragment-->
            <!--xmlns:android="http://schemas.android.com/apk/res/android"-->
            <!--android:id = "@+id/map"-->
            <!--android:layout_width="fill_parent"-->
            <!--android:layout_height="fill_parent"-->
            <!--android:name="com.google.android.gms.maps.MapFragment"/>-->
            <android.support.design.widget.CoordinatorLayout
                android:id="@+id/snackbar"
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:layout_gravity="bottom"
                android:background="#ffffff"
                >
            </android.support.design.widget.CoordinatorLayout>
            <com.mapbox.mapboxsdk.views.MapView
                android:id="@+id/map"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                mapbox:access_token="@string/accessToken"
                >
                <ImageButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="top|left"
                    android:src="@drawable/ic_drawer"
                    android:alpha="0.80"
                    android:id="@+id/drawerbutton"/>
                <Button
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|start"
                    android:alpha="0.80"
                    android:text="Re-Center"
                    android:id="@+id/recenter"/>
            </com.mapbox.mapboxsdk.views.MapView>
            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/imageHolder1">
            </FrameLayout>
        </FrameLayout>
        <!--<android.support.design.widget.FloatingActionButton-->
            <!--android:id="@+id/fab"-->
            <!--android:layout_width="wrap_content"-->
            <!--android:layout_height="wrap_content"-->
            <!--android:layout_gravity="bottom|end"-->
            <!--android:layout_marginBottom="80dp"-->
            <!--android:layout_marginEnd="12dp"-->
            <!--android:src="@drawable/fab"-->
            <!--app:borderWidth="0dp"-->
            <!--app:fabSize="normal"-->
            <!--app:rippleColor="#c1c1c1"-->
            <!--/>-->
        <FrameLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|end"
            android:layout_marginBottom="80dp"
            android:layout_marginRight="12dp">
            <android.support.design.widget.FloatingActionButton
                android:id="@+id/fab"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@android:color/transparent"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="Queue"
                android:elevation="16dp"
                android:textColor="@android:color/white"
                android:textAppearance="?android:attr/textAppearanceMedium"/>
        </FrameLayout>
        <com.bowyer.app.fabtransitionlayout.BottomSheetLayout
            android:id="@+id/bottom_sheet"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="bottom"
            app:ft_container_gravity="center"
            app:ft_color="#616161">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="450dp"
                android:id="@+id/maxHeight"
                android:alpha="0.95"
                >
                <ListView
                    android:id="@+id/list_menu"
                    android:background="#ffffff"
                    android:divider="#ffffff"
                    android:dividerHeight="2dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
            </LinearLayout>

        </com.bowyer.app.fabtransitionlayout.BottomSheetLayout>

    </FrameLayout>


</RelativeLayout>
